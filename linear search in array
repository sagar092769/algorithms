import java.util.*;
public class Main {
    public static void main(String args[]) {
        // Your Code Here
		Scanner scn=new Scanner(System.in);
		int n=scn.nextInt();
		int arr[]=new int[n];
		for(int i=0;i<n;i++){
			arr[i]=scn.nextInt();
		}
		int m=scn.nextInt();
		for(int i=0;i<n;i++){
			if(arr[i]==m){
				System.out.println(i);
				return ;
			}
		}
		System.out.println("-1");

    }
}
