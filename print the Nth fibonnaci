import java.util.*;
public class Main {
    public static void main(String args[]) {
        // Your Code Here
        Scanner scn=new Scanner(System.in);
        int n=scn.nextInt();
        int a=0;
        int b=1;
         int c=0;
        for(int x=1;x<=n-1;x++){
           c=a+b;
           a=b;
           b=c;
        }
        System.out.println(c);
    }
}
